//

public int[] SortedSquaredArray(int[] array) 
    {
        int[] SquaredArray = new int[array.Length] ;
        for(int i=0; i<array.Length; i++)
        {
            int value = array[i];
            SquaredArray[i]= value*value;
        }
        Array.Sort(SquaredArray);
	    return  SquaredArray;
	}
