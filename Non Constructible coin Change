//int[] input = { 1,2,5};

//Time complexity - O(nlogn) , Space complexity - O(1)

int NonConstructibleChange(int[] coins) 
    {
		Array.Sort(coins);
        int change=0;
        foreach(int coin in coins)
        {
            if(coin>change+1)
            {
                return change+1;
            }
            change= change+coin;
        }
        
		return change+1;
	}
